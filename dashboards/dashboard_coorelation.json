      "panel-1": {
        "kind": "Panel",
        "spec": {
          "data": {
            "kind": "QueryGroup",
            "spec": {
              "queries": [
                {
                  "kind": "PanelQuery",
                  "spec": {
                    "hidden": false,
                    "query": {
                      "datasource": {
                        "name": "cf5409xuhophce"
                      },
                      "group": "grafana-postgresql-datasource",
                      "kind": "DataQuery",
                      "spec": {
                        "dataset": "air_quality_db",
                        "editorMode": "code",
                        "format": "table",
                        "rawQuery": true,
                        "rawSql": "SELECT\r\n  -- 1. X-Axis Data: Average Temperature for the hour\r\n  AVG(CASE WHEN meta.parameter = 'Temprature' THEN aqs.sensor_value END) AS \"Temperature (C)\",\r\n\r\n  -- 2. Y-Axis Data: Average PM2.5 for the hour\r\n  AVG(CASE WHEN meta.parameter = 'PM2.5' THEN aqs.sensor_value END) AS \"PM2.5 (ug/m3)\",\r\n\r\n  -- 3. Point Name (Label)\r\n  loc.station_name AS \"Station\"\r\n\r\nFROM air_quality_all_sensors aqs\r\nJOIN sensor_readings_metadata meta ON aqs.sensor_id = meta.sensor_id\r\nJOIN location_metadata loc ON aqs.location_id = loc.location_id\r\n\r\nWHERE\r\n  -- Standard Time Filter\r\n  $__timeFilter(aqs.sensor_timestamp_utc)\r\n  \r\n  -- Filter by City Variable\r\n  AND loc.city IN (${city:singlequote})\r\n  \r\n  -- Hardcode the parameters we need for X and Y axes\r\n  -- (We ignore the $parameter variable here to ensure the chart always works)\r\n  AND meta.parameter IN ('Temprature', 'PM2.5')\r\n\r\n-- Group by Hour and Station to align the data points\r\nGROUP BY \r\n  loc.station_name,\r\n  date_trunc('hour', aqs.sensor_timestamp_utc)\r\n\r\n-- Remove empty rows (where we only had Temp but no PM2.5, or vice versa)\r\nHAVING \r\n  AVG(CASE WHEN meta.parameter = 'Temprature' THEN aqs.sensor_value END) IS NOT NULL\r\n  AND \r\n  AVG(CASE WHEN meta.parameter = 'PM2.5' THEN aqs.sensor_value END) IS NOT NULL;",
                        "sql": {
                          "columns": [
                            {
                              "parameters": [],
                              "type": "function"
                            }
                          ],
                          "groupBy": [
                            {
                              "property": {
                                "type": "string"
                              },
                              "type": "groupBy"
                            }
                          ],
                          "limit": 50
                        }
                      },
                      "version": "v0"
                    },
