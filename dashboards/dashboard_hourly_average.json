              "queries": [
                {
                  "kind": "PanelQuery",
                  "spec": {
                    "hidden": false,
                    "query": {
                      "datasource": {
                        "name": "cf5409xuhophce"
                      },
                      "group": "grafana-postgresql-datasource",
                      "kind": "DataQuery",
                      "spec": {
                        "dataset": "air_quality_db",
                        "editorMode": "code",
                        "format": "time_series",
                        "rawQuery": true,
                        "rawSql": "SELECT\r\n  -- 1. Create a \"Fake\" Timestamp for Today + The Hour\r\n  -- This tricks Grafana into treating \"13\" as \"Today at 1:00 PM\"\r\n  (CURRENT_DATE + (EXTRACT(HOUR FROM aqs.sensor_timestamp_utc) || ' hours')::interval) AS \"time\",\r\n  \r\n  -- 2. The Value\r\n  AVG(aqs.sensor_value) AS \"value\",\r\n  \r\n  -- 3. The Series Name (City)\r\n  loc.city AS \"metric\"\r\n  \r\nFROM air_quality_all_sensors aqs\r\nJOIN location_metadata loc ON aqs.location_id = loc.location_id\r\nJOIN sensor_readings_metadata meta ON aqs.sensor_id = meta.sensor_id\r\n\r\nWHERE\r\n  $__timeFilter(aqs.sensor_timestamp_utc)\r\n  AND loc.city IN (${city:singlequote})\r\n  AND meta.parameter IN (${parameter:singlequote})\r\n  \r\nGROUP BY 1, 3\r\nORDER BY 1 ASC;",
                        "sql": {
